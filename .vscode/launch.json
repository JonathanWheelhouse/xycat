    "version": "0.2.0",
    "configurations": [
        {
            "name": "xcat .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/xcat/bin/netcoreapp3.1/xcat.dll",
            "args": [],
            "cwd": "${workspaceFolder}/xcat",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "ycat .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ycat/bin/netcoreapp3.1/ycat.dll",
            "args": [],
            "cwd": "${workspaceFolder}/xcat",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
